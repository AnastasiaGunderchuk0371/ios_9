// Протокол с методом и свойствами
protocol SomeProtocol {
    var requiredProperty: String { get }
    var optionalProperty: Int? { get set }
    func someMethod()
}

// Класс, подписанный на протокол и реализующий только одно обязательное свойство
class SomeClass: SomeProtocol {
    var requiredProperty: String = "Значение свойства"
    var optionalProperty: Int?
    func someMethod() {
        // Реализация метода
    }
}
